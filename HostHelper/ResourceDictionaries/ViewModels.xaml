<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:local="clr-namespace:UI.WPF.ResourceDictionaries"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:UI.WPF.Views"
                    xmlns:vm="clr-namespace:ApacheLib.ViewModels;assembly=ApacheLib">
    <vm:MainWindowVM x:Key="mvm"/>
    
    <DataTemplate DataType="{x:Type vm:VirtualHostVM}">
        <v:VirtualHostView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:HostFileEntryVM}">
        <v:HostFileEntryView />
    </DataTemplate>
    
</ResourceDictionary>